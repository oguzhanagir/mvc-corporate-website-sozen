@model IEnumerable<EntityLayer.Concrete.Category>
@{
    ViewBag.Title = "Index";
}


<h2>Burası Index</h2>
<br />

<table>
    <tr>
        <th>ID</th>
        <th>Kategori Adı</th>
    </tr>
    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.CategoryID</td>
                    <td>@item.CategoryName</td>
                </tr>
            }
</table>
